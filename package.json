{
  "name": "makerset-receipt-hub-postgres",
  "version": "1.0.0",
  "description": "MakerLab Sets Management System with SQLite",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "setup": "npm install && cd server && npm install && cd ../client && npm install",
    "db:setup": "cd server && npm run db:setup",
    "db:migrate": "cd server && npm run db:migrate",
    "db:seed": "cd server && npm run db:seed"
  },
  "keywords": [
    "makerset",
    "receipt",
    "sqlite",
    "multilingual",
    "education"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@mui/icons-material": "^7.3.7",
    "node-cron": "^4.2.1",
    "puppeteer": "^24.26.0",
    "react-router-dom": "^7.12.0",
    "sharp": "^0.34.4"
  }
}
